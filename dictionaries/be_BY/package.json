{
  "name": "@cspell/dict-be-by",
  "version": "1.0.0",
  "description": "Belarusian dictionary for cspell. -- Private until verified",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 cspell-tools-cli build",
    "sync": "pnpm cpy \"node_modules/dictionary-be/**\" src/hunspell",
    "test": "head -n 1000 \"src/be_BY.txt\" | cspell -v -c ./cspell-ext.json \"--locale=be\" \"--languageId=*\" stdin",
    "prepublishOnly": "echo OK",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "cspell-ext",
    "be_BY",
    "Belarusian",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/dictionaries/be_BY#readme",
  "devDependencies": {
    "dictionary-be": "^1.0.0"
  },
  "dependencies": {},
  "files": [
    "dict/be-by.trie",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
